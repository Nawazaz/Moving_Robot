//Directions:
DDRD =  0b00100000; //Left forward
PORTD ^= 0b00100000;//Left forward
DDRD = 0b01000000; //Right forward
PORTD ^= 0b01000000;//Right forward
DDRD =  0b01100000;//Both side forward
PORTD ^= 0b01100000;//Both side forward
DDRD =  0b00000100; //backwards left
PORTD ^= 0b00000100; //backwards left
DDRD =  0b00010000;//Backwards right
PORTD ^= 0b00010000;//Backwards right
DDRB = 0b00000010; //Extra LED light
PORTB ^= 0b00000010; //Extra LED light
